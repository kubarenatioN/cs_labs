<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:lab4.Styles">

    <Color x:Key="MainForeground">#1E1824</Color>
    <SolidColorBrush Color="{StaticResource MainForeground}" x:Key="MainForegroundBrush"/>

    <Color x:Key="MainBackground">#C1F0EA</Color>
    <SolidColorBrush Color="{StaticResource MainBackground}" x:Key="MainBackgroundBrush"/>

    <Color x:Key="MainForegroundHighContrast">#000</Color>
    <SolidColorBrush Color="{StaticResource MainForegroundHighContrast}" x:Key="MainForegroundHighContrastBrush"/>

    <Color x:Key="MainBackgroundHighContrast">#D8F0EA</Color>
    <SolidColorBrush Color="{StaticResource MainBackgroundHighContrast}" x:Key="MainBackgroundHighContrastBrush"/>

    <Color x:Key="MainForegroundDark">#352F3B</Color>
    <SolidColorBrush Color="{StaticResource MainForegroundDark}" x:Key="MainForegroundDarkBrush"/>

    <Color x:Key="MainBackgroundDark">#97CCC6</Color>
    <SolidColorBrush Color="{StaticResource MainBackgroundDark}" x:Key="MainBackgroundDarkBrush"/>

</ResourceDictionary>